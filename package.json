{
  "name": "unrun",
  "version": "0.2.13",
  "packageManager": "pnpm@10.22.0",
  "description": "A tool to load and execute any JavaScript or TypeScript code at runtime.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://gugustinette.github.io/unrun/",
  "bugs": {
    "url": "https://github.com/Gugustinette/unrun/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gugustinette/unrun.git"
  },
  "author": "Augustin Mercier <gugustinette@proton.me>",
  "funding": "https://github.com/sponsors/Gugustinette",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "bin": {
    "unrun": "./dist/cli.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --no-cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "pretest": "node ./scripts/manage-pnpm-override.mjs add",
    "test": "vitest",
    "posttest": "node ./scripts/manage-pnpm-override.mjs remove",
    "pretest:run": "node ./scripts/manage-pnpm-override.mjs add",
    "test:run": "vitest run",
    "posttest:run": "node ./scripts/manage-pnpm-override.mjs remove",
    "ecosystem-ci": "ecosystem-ci",
    "ecosystem-ci:force": "ecosystem-ci --force",
    "test:update-fixtures": "node ./dist/cli.mjs ./scripts/update-fixtures.ts",
    "benchmark": "tsdown -c benchmark/tsdown.config.ts && node ./benchmark/dist/benchmark.mjs",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate-reference": "node ./dist/cli.mjs ./docs/.vitepress/scripts/generate-reference.ts"
  },
  "peerDependencies": {
    "synckit": "^0.11.11"
  },
  "peerDependenciesMeta": {
    "synckit": {
      "optional": true
    }
  },
  "dependencies": {
    "@oxc-project/runtime": "^0.99.0",
    "rolldown": "1.0.0-beta.52"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.1.1",
    "@sxzz/eslint-config": "^7.3.2",
    "@sxzz/prettier-config": "^2.2.5",
    "@types/node": "^24.10.1",
    "acorn": "^8.15.0",
    "bumpp": "^10.3.2",
    "bundle-require": "^5.1.0",
    "config": "^4.1.1",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "estree-walker": "^3.0.3",
    "etag": "^1.8.1",
    "fast-glob": "^3.3.3",
    "giget": "^2.0.0",
    "is-installed-globally": "^1.0.0",
    "jiti": "^2.6.1",
    "mime": "^4.1.0",
    "moment-timezone": "^0.6.0",
    "nano-jsx": "^0.2.1",
    "preact": "^10.27.2",
    "preact-render-to-string": "^6.6.3",
    "prettier": "^3.7.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "synckit": "^0.11.11",
    "test-ecosystem-ci": "^0.0.1",
    "tinyexec": "^1.0.2",
    "tsdown": "^0.16.7",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.9.3",
    "unconfig": "^7.4.1",
    "unplugin-vue": "^7.0.8",
    "vite": "^7.2.4",
    "vitepress": "2.0.0-alpha.12",
    "vitepress-plugin-group-icons": "^1.6.5",
    "vitest": "4.0.8",
    "vue": "^3.5.25",
    "vue-tsc": "^3.1.5",
    "zod": "^4.1.13"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "pnpm": {
    "overrides": {
      "@prettier/plugin-oxc": "^0.1.1"
    }
  },
  "prettier": "@sxzz/prettier-config"
}
