{
  "name": "unrun",
  "version": "0.0.0",
  "packageManager": "pnpm@10.15.0",
  "description": "Load anything at runtime",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/gugustinette/unrun#readme",
  "bugs": {
    "url": "https://github.com/gugustinette/unrun/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gugustinette/unrun.git"
  },
  "author": "Augustin Mercier <gugustinette@proton.me>",
  "funding": "https://github.com/sponsors/gugustinette",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && unotp pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.1.4",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.3.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config",
  "dependencies": {
    "rolldown": "1.0.0-beta.37"
  }
}
