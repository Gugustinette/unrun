{
  "name": "unrun",
  "version": "0.0.0",
  "packageManager": "pnpm@10.15.0",
  "description": "Load anything at runtime",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/gugustinette/unrun#readme",
  "bugs": {
    "url": "https://github.com/gugustinette/unrun/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gugustinette/unrun.git"
  },
  "author": "Augustin Mercier <gugustinette@proton.me>",
  "funding": "https://github.com/sponsors/gugustinette",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "unrun": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:update-fixtures": "tsx ./scripts/update-fixtures.ts",
    "benchmark": "tsx ./scripts/benchmark.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && unotp pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@oxc-project/runtime": "^0.95.0",
    "cac": "^6.7.14",
    "rolldown": "1.0.0-beta.44",
    "synckit": "^0.11.11"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.1.4",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.3.0",
    "acorn": "^8.15.0",
    "bumpp": "^10.2.3",
    "bundle-require": "^5.1.0",
    "config": "^4.1.1",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "esbuild": "^0.25.10",
    "eslint": "^9.34.0",
    "estree-walker": "^3.0.3",
    "etag": "^1.8.1",
    "fast-glob": "^3.3.3",
    "giget": "^2.0.0",
    "is-installed-globally": "^1.0.0",
    "jiti": "^2.6.1",
    "mime": "^4.1.0",
    "moment-timezone": "^0.6.0",
    "nano-jsx": "^0.2.0",
    "preact": "^10.27.2",
    "preact-render-to-string": "^6.6.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reflect-metadata": "^0.2.2",
    "tinyexec": "^1.0.1",
    "tsdown": "^0.15.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "4.0.2",
    "vue": "^3.5.22",
    "zod": "^4.1.8"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
